{
	"name": "baraja-core/package-manager",
	"description": "Search all package dependencies automatically and register to your project.",
	"homepage": "https://github.com/baraja-core/package-manager",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"ext-PDO": "*",
		"ext-json": "*",
		"nette/di": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/application": "^3.0",
		"nette/neon": "^3.0",
		"nette/finder": "^2.5",
		"contributte/console": "^0.8.0"
	},
	"require-dev": {
		"nette/bootstrap": "^3.0",
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"post-autoload-dump": "Baraja\\PackageManager\\PackageRegistrator::composerRenderCiDetectorInfo",
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
